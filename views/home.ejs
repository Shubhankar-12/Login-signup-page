<%-include("header")-%>
<div class="card">
  <div class="img-bx">
    <%if(user.image){%>
    <img src="<%='/uploads/'+user.image%>" alt="img" />
    <%}else{%>
    <img src="/asset/userphoto.png" alt="" />
    <%}%>
  </div>
  <div class="content">
    <div class="detail">
      <h2><%=user.fullname%><br /><span><%=user.usrNum%></span></h2>
      <ul class="sci">
        <li>
          <a href="mailto:<%=user.username%>?subject=from login page"
            ><i class="fab fa-google"></i
          ></a>
        </li>
      </ul>
    </div>
  </div>
</div>
<%-include("footer")-%>
